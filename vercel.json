{
  "functions": {
    "api/send-email.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/images/(.*)",
      "destination": "/images/$1"
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
